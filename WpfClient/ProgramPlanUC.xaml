<UserControl x:Class="WpfClient.ProgramPlanUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfClient"
             mc:Ignorable="d" d:DesignHeight="140" d:DesignWidth="1120">
    <Grid Width="1120">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <StackPanel x:Name="PlanNameSP" Grid.Column="0"/>
        <StackPanel x:Name="SundaySP" Grid.Column="1"/>
        <StackPanel x:Name="MondaySP" Grid.Column="2"/>
        <StackPanel x:Name="TuesdaySP" Grid.Column="3"/>
        <StackPanel x:Name="WednesdaySP" Grid.Column="4"/>
        <StackPanel x:Name="ThursdaySP" Grid.Column="5"/>
        <StackPanel x:Name="FridaySP" Grid.Column="6"/>
        <StackPanel x:Name="SaturdaySP" Grid.Column="7"/>
    </Grid>
</UserControl>
